<?xml version="1.0"?>
<!--Phoronix Test Suite v10.8.4-->
<PhoronixTestSuite>
  <TestInformation>
    <Title>Network monitoring</Title>
    <Description>This test monitors the bandwidth of a selected network interface.</Description>
    <TimesToRun>1</TimesToRun>
  </TestInformation>
  <TestProfile>
    <Version>1.0.0</Version>
    <SupportedPlatforms>Linux</SupportedPlatforms>
    <SoftwareType>Utility</SoftwareType>
    <TestType>System</TestType>
    <License>Free</License>
    <Status>Verified</Status>
    <EnvironmentSize>0.01</EnvironmentSize>
    <Maintainer>Savoir-faire Linux</Maintainer>
  </TestProfile>
  <TestSettings>
    <Default>
      <AllowCacheShare>TRUE</AllowCacheShare>
      <DisplayName>Monitored interface</DisplayName>
      <Identifier>interface</Identifier>
    </Default>
    <Option>
      <Menu>
        <Entry>
          <Name>Interface</Name>
          <Value>-i $interface</Value>
        </Entry>
      </Menu>
    </Option>
  </TestSettings>
</PhoronixTestSuite>
